{
  "license": "MIT",
  "name": "eslint-plugin-roku",
  "repository": "git@github.com:RokuRoad/eslint-plugin-roku.git",
  "version": "1.5.1",
  "description": "The ESLint custom plugin with rules and parser for .brs files",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "linter",
    "brightscript",
    "brs",
    "roku",
    "roku-development"
  ],
  "author": "Igor Alpert",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "release": "tsc && semantic-release",
    "test": "jest",
    "coverage": "jest --coverage",
    "prepublish": "tsc",
    "prepare": "tsc",
    "update-contrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'"
  },
  "peerDependencies": {
    "eslint": "5.6.0"
  },
  "dependencies": {
    "@roku-road/bright": "^1.3.2"
  },
  "devDependencies": {
    "@types/eslint": "4.16.3",
    "@types/jest": "23.3.3",
    "@types/node": "^11.9.0",
    "eslint": "^7.17.0",
    "jest": "23.6.0",
    "semantic-release": "15.9.16",
    "ts-jest": "23.10.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "3.0.3",
    "weallcontribute": "1.0.9"
  },
  "engines": {
    "node": ">=9.10.0"
  }
}
